		}
		
		// 省注协关于非执业会员焕发新证书公告
		if(partId=="xzsgg"){
			document.getElementById("xzsgg1").style.display = "none";
			document.getElementById("xzsgg2").style.display = "";
		}
		
	}
	
	
	function f_downType(typeid,pages){
		var typeid_value = document.getElementById(typeid+"_value").value*1;
		if(typeid_value>pages){
			typeid_value = pages;
			document.getElementById(typeid+"_value").value = pages;
		}
		if(typeid_value<1){
			typeid_value = 1;	
			document.getElementById(typeid+"_value").value = 1;
		}
		if(typeid_value<pages){
			for(var i=1;i<(pages+1);i++){
				if(typeid_value*1+1==i){
					document.getElementById(typeid+(typeid_value+1)).style.display = "";
				}else{
					document.getElementById(typeid+i).style.display = "none";
				}
			}
			if(typeid_value>=pages){
				document.getElementById(typeid+"_value").value = typeid_value*1;
			}else{
				document.getElementById(typeid+"_value").value = typeid_value*1 + 1;
			}
		}
	}
	
	//上一页
	function f_up(){
		// 注册会计师全国统一考试报名
		if(partId=="ksbm"){
