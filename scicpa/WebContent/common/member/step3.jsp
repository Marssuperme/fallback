		}
	}
	
	//下一页
	function f_down(){
		// 注册会计师全国统一考试报名
		if(partId=="ksbm"){
			document.getElementById("ksbm1").style.display = "none";
			document.getElementById("ksbm2").style.display = "";
		}
		
		// 注册会计师转所网上操作步骤
		if(partId=="kjszsczbz"){
			f_downType("kjszsczbz",5);
		}
		
		
		// 申请注册会计师注册网上操作步骤
		if(partId=="sqzcczbz"){
			f_downType("sqzcczbz",4);
		}
		
		// 注册会计师注册
		if(partId=="zckjszc"){
			f_downType("zckjszc",3);
		}
		
		// 注册会计师转所
		if(partId=="zckjszs"){
			f_downType("zckjszs",3);
		}
		
		// 个人会员信息变更
		if(partId=="grxxbd"){
			document.getElementById("grxxbd1").style.display = "none";
			document.getElementById("grxxbd2").style.display = "";
		}
		
		// 会计事务所信息变更
		if(partId=="swsxxbg"){
			f_downType("swsxxbg",3);
		}
		
		// 拟担任会计事务所股东或合伙人注册会计师办理从事审计业务证明
		if(partId=="sjywzm"){
