		}
		
		// 个人会员信息变更
		if(partId=="grxxbd"){
			document.getElementById("grxxbd1").style.display = "";
			document.getElementById("grxxbd2").style.display = "none";
		}
		
		// 会计事务所信息变更
		if(partId=="swsxxbg"){
			f_upType("swsxxbg",3);
		}
		
		// 拟担任会计事务所股东或合伙人注册会计师办理从事审计业务证明
		if(partId=="sjywzm"){
			f_upType("sjywzm",3);
		}
		
		
		// 省注协关于非执业会员焕发新证书公告
		if(partId=="xzsgg"){
			document.getElementById("xzsgg1").style.display = "";
			document.getElementById("xzsgg2").style.display = "none";
		}
		
		
	}
	
	function f_upType(typeid,pages){
		var typeid_value = document.getElementById(typeid+"_value").value*1;
		if(typeid_value>pages){
			typeid_value = pages;
			document.getElementById(typeid+"_value").value = pages;
		}
		if(typeid_value<1){
			typeid_value = 1;	
			document.getElementById(typeid+"_value").value = 1;
		}
		if(typeid_value>1){
			for(var i=1;i<(pages+1);i++){
				if(typeid_value-1==i){
					document.getElementById(typeid+(typeid_value-1)).style.display = "";
				}else{
					document.getElementById(typeid+i).style.display = "none";
				}
			}
			if(typeid_value<=1){
				document.getElementById(typeid+"_value").value = 1;
			}else{
				document.getElementById(typeid+"_value").value = typeid_value*1 - 1;
			}
		}
	}
	
